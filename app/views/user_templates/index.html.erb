<div class="row">
  <div class="col-md-8 offset-md-2">
    <div class="card">
      <div class="card-header">
        <h3 class="card-title">
          All Users
        </h3>
      </div>

      <% @users.each do |user| %>

        <ul class="list-group">
          <li class="list-group-item">
            <a href="/users/<%= user.id %>">
              <%= user.username %>
            </a>
          </li>
          

        <% end %>

      </div>
    </div>
  </div>
